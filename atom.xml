</span><span class='line'>  module Strategies
</span><span class='line'>    class Icps &lt; OmniAuth::Strategies::OAuth2
</span><span class='line'>
</span><span class='line'>      CUSTOM_PROVIDER_URL = 'http://192.168.0.201:3000'
