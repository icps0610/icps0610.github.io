</span><span class='line'>            <span class="n">c</span> <span class="o">&lt;</span> <span class="mi">127</span> <span class="o">?</span> <span class="mi">1</span> <span class="p">:</span> <span class="mi">2</span>
</span><span class='line'>        <span class="k">end</span><span class="o">.</span><span class="n">compact</span><span class="o">.</span><span class="n">inject</span><span class="p">(</span><span class="ss">:+</span><span class="p">)</span>
