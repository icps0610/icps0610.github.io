</span><span class='line'>    <span class="k">if</span> <span class="nb">string</span><span class="p">(</span><span class="nx">num</span><span class="p">[</span><span class="mi">6</span><span class="p">])</span> <span class="o">==</span> <span class="s">&quot;7&quot;</span> <span class="p">{</span>
